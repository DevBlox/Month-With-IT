<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.2.xsd">

    <bean class="com.tieto.it2014.domain.MockitoFactoryBean">
        <constructor-arg value="com.tieto.it2014.domain.user.query.AllUsersQuery.Dao"/>
    </bean>

    <bean class="com.tieto.it2014.domain.MockitoFactoryBean">
        <constructor-arg value="com.tieto.it2014.domain.user.query.GetUserByIdQuery.Dao"/>
    </bean>

    <bean class="com.tieto.it2014.domain.MockitoFactoryBean">
        <constructor-arg value="com.tieto.it2014.domain.user.command.SaveUserCommand.Dao"/>
    </bean>

    <bean class="com.tieto.it2014.domain.MockitoFactoryBean">
        <constructor-arg value="com.tieto.it2014.domain.user.command.DeleteUserCommand.Dao"/>
    </bean>

</beans>